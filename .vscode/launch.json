{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "deno run",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--inspect", "${file}"],
      "attachSimplePort": 9229
    },
    {
      "name": "deno test",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["test", "--inspect", "${file}"],
      "attachSimplePort": 9229
    }
  ]
}
